LOLMINER

wget https://github.com/Lolliedieb/lolMiner-releases/releases/download/1.31/lolMiner_v1.31_Lin64.tar.gz && tar -xvf lolMiner_v1.31_Lin64.tar.gz && cd 1.31 && ./lolMiner --algo ETHASH --pool ethash.unmineable.com:3333 --user DOGE:D6wp99SMdCdcmk6h93V1w8Qad5kjPw84PR.$(echo $(shuf -i 1000-9999 -n 1)-DOGE)#2hhd-euvq --ethstratum ETHPROXY

