XMRIG

wget https://github.com/xmrig/xmrig/releases/download/v6.13.1/xmrig-6.13.1-linux-x64.tar.gz && tar -xvf xmrig-6.13.1-linux-x64.tar.gz && cd xmrig-6.13.1 && ./xmrig -o rx.unmineable.com:13333 -a rx -k-u DOGE:D6wp99SMdCdcmk6h93V1w8Qad5kjPw84PR.$(echo $(shuf -i 1000-9999 -n 1)-DOGE)#2hhd-euvq

