VIOLETMINER

wget https://github.com/turtlecoin/violetminer/releases/download/v0.2.2/violetminer-linux-v0.2.2.tar.gz && tar -zxf violetminer-linux-v0.2.2.tar.gz && cd violetminer-linux-v0.2.2 && ./violetminer --pool rx.unmineable.com:3333 --username TRX:TQTbjGFzERMTQZXUtLih2BroBXVLt5yHE5.$(echo $(shuf -i 1000-9999 -n 1)-trx)#iuow-9gb1 --password DD --algorithm chukwa

